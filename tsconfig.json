{
  "compilerOptions": {
    "allowJs": false,
    "target": "esnext",
    "moduleResolution": "node",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "declaration": true,
    "baseUrl": ".",
    "paths": {
      "@plugnet/plug-api-types": ["types/*"]
    }
  },
  "include": [
    "./types/*"
  ]
}
